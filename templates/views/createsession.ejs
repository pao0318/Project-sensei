<!-- Session forms -->

<div class="card border-primary mb-3 mx-4" style="max-width: 25rem;">
  <div class="card-body text-primary">
    <h4>Create a session</h4>
      <form role="form" class="text-start" action="/createsession" method="POST">
        <div class="input-group input-group-outline my-3">
          <input type="name" class="form-control" placeholder="Session Name" name="name"  required>
        </div>
        <div class="input-group input-group-outline mb-3">
          <input type="date" class="form-control " placeholder="Date" name="date"  required>
        </div>
        <div class="input-group input-group-outline mb-3">
          <div class="input-group-md input-group-outline mb-3 " style="border:2px solid rgb(235, 235, 235);margin-right: 8px; border-radius: 10px;">
            <label >Select a Topic</label>
            <br/>
            <select class="p-1" name="description">
              <option value="Coding">Coding</option>
              <option value="Machine learning">Machine Learning</option>
              <option value="DevOps">DevOps</option>
              <option value="Development">Development</option>
              <option value="Design">Design</option>
            </select>
          </div>
        </div>
        <div class="text-center">
          <button type="submit" class="btn bg-gradient-primary w-50 my-4 mb-2">Create Session</button>
        </div>
      </form>
  </div>
</div>

<!-- End of session forms -->
<div class="container">
  <div class="row">
    <h4 class="text-muted">Sessions you have created</h4>
    <% for (let i =0;i<session.length;i++) { %>
          <div class="col-4 col-md-4"><div class="card" style="width: 23rem; display: flex; margin-bottom: 20px;">
            <div class="card-body">
              <h5 class="card-title"><%= session[i].name %></h5>
              <p class="card-text">Specialization : <%= session[i].description %></p>
              <a href="#" class="btn btn-primary">Book Session</a>
            </div>
          </div></div>
    <% } %>
  </div>
</div>

